<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.demo.Controller.MenuController"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <TitledPane animated="false" prefHeight="350.0" prefWidth="400.0" text="Главная страница">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <children>
                        <Button fx:id="employeeBtn" onAction="#openEmployeePage"  layoutX="106.0" layoutY="110.0" mnemonicParsing="false" prefHeight="46.0" prefWidth="185.0" text="СОТРУДНИКИ" />
                        <Button fx:id="departmentBtn" onAction="#openDepartmentPage" layoutX="106.0" layoutY="166.0" mnemonicParsing="false" prefHeight="46.0" prefWidth="185.0" text="ОТДЕЛЫ" />
                    </children>
                </AnchorPane>
            </content>
        </TitledPane>
    </children>
</AnchorPane>
